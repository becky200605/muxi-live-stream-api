{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/library/inlibrary": {
            "post": {
                "description": "借助用户Cookie信息查询用户当前是否在图书馆内",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "用户"
                ],
                "summary": "查询用户是否在图书馆",
                "parameters": [
                    {
                        "type": "string",
                        "description": "用户认证Cookie",
                        "name": "Cookie",
                        "in": "header",
                        "required": true
                    },
                    {
                        "description": "请求参数",
                        "name": "req",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/types.InLibraryRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "查询成功返回用户在馆状态",
                        "schema": {
                            "$ref": "#/definitions/types.Response"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "$ref": "#/definitions/types.Response"
                        }
                    },
                    "401": {
                        "description": "未授权访问",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/library/login": {
            "post": {
                "description": "用户通过学号和密码进行登录，获取访问令牌",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "用户"
                ],
                "summary": "用户登录",
                "parameters": [
                    {
                        "description": "登录请求参数",
                        "name": "req",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/types.LoginRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "登录成功返回信息",
                        "schema": {
                            "$ref": "#/definitions/types.Response"
                        }
                    },
                    "400": {
                        "description": "登录失败返回信息",
                        "schema": {
                            "$ref": "#/definitions/types.Response"
                        }
                    }
                }
            }
        },
        "/library/roomids": {
            "get": {
                "description": "获取系统中所有活跃的房间ID列表",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "图书馆信息"
                ],
                "summary": "获取所有房间ID",
                "parameters": [
                    {
                        "type": "string",
                        "description": "用户认证Cookie",
                        "name": "Cookie",
                        "in": "header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "成功返回房间ID列表",
                        "schema": {
                            "$ref": "#/definitions/types.Response"
                        }
                    },
                    "400": {
                        "description": "请求参数错误",
                        "schema": {
                            "$ref": "#/definitions/types.Response"
                        }
                    },
                    "401": {
                        "description": "未授权访问",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/library/seatinfo": {
            "post": {
                "description": "获取系统中所有座位的信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "图书馆信息"
                ],
                "summary": "获取所有座位信息",
                "parameters": [
                    {
                        "type": "string",
                        "description": "用户认证Cookie",
                        "name": "Cookie",
                        "in": "header"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "返回所有座位信息",
                        "schema": {
                            "$ref": "#/definitions/types.Response"
                        }
                    },
                    "401": {
                        "description": "用户未授权",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "types.InLibraryRequest": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "room_ids": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "types.LoginRequest": {
            "type": "object",
            "properties": {
                "password": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "types.Response": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "data": {},
                "message": {
                    "type": "string"
                }
            }
        }
    }
}